{
  "name": "Mouseflow DevTools",
  "version": "1.0",
  "description": "Developer tools for Mouseflow",
  "permissions": ["activeTab", "storage"],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["pageScript.js"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/mf_inactive.png"
  },
  "manifest_version": 2
}